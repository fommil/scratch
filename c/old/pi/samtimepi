#----------
CCPREFIX=kg
OPTLEVEL=0
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" pi.c $OPT -o pi-${CCPREFIX}cc${OPT}.sam

OPTLEVEL=1
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" pi.c $OPT -o pi-${CCPREFIX}cc${OPT}.sam

OPTLEVEL=2
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" pi.c $OPT -o pi-${CCPREFIX}cc${OPT}.sam

OPTLEVEL=3
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" pi.c $OPT -o pi-${CCPREFIX}cc${OPT}.sam

OPTLEVEL=s
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" pi.c $OPT -o pi-${CCPREFIX}cc${OPT}.sam

