#----------
CCPREFIX=/opt/intel/compiler50/ia32/bin/i
PREFIX=i
OPTLEVEL=0
OPT=-O${OPTLEVEL}
ADDITIONALINCLUDES='-I/opt/intel/compiler50/ia32/include -i_dynamic'
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" $ADDITIONALINCLUDES pi.c $OPT -o pi-${PREFIX}cc${OPT}.sam

OPTLEVEL=1
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" $ADDITIONALINCLUDES pi.c $OPT -o pi-${PREFIX}cc${OPT}.sam

OPTLEVEL=2
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" $ADDITIONALINCLUDES pi.c $OPT -o pi-${PREFIX}cc${OPT}.sam

OPTLEVEL=3
OPT=-O${OPTLEVEL}
echo "Timing ${CCPREFIX}cc to compile with $OPT:"
time -f %e "${CCPREFIX}cc" $ADDITIONALINCLUDES $OPT pi.c -o pi-${PREFIX}cc${OPT}.sam

