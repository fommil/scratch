  coeff(convert(series(-I*exp(expnt),p[pval],2),polynom),p[pval])),pt=0)
))/exp(x[1]*f[1]+x[2]*f[2]+x[3]*f[3]+x[4]*f[4]));
  end proc:</Font></Text-field></Input></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input"><Font opaque="false">fstarx_analytic_expand:=proc(xg)
